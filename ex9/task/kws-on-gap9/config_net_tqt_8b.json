{
    "description": {},
    "data": {
        "train": {
            "dataset": {
                "transform": {
                    "class": "TransformA",
                    "kwargs": {
                        "augment": false
                    }
                }
            },
            "dataloader": {
                "bs": 256
            }
        },
        "valid": {
            "dataset": {
                "transform": {
                    "class": "TransformA",
                    "kwargs": {
                        "augment": false
                    }
                }
            },
            "dataloader": {
                "bs": 256
            }

        }
    },
    "network": {
        "class": "DSCNN",
        "kwargs": {
            "config": "standard",
            "capacity": 1.0
        },
        "quantize": {
            "function": "pact_recipe",
            "kwargs": {
                "config": {
                    "PACTConv2d": {
                        "stem_block.conv$": {},
                        "conv_blocks.0.conv_dw$": {},
                        "conv_blocks.0.conv_pw$": {},
                        "conv_blocks.1.conv_dw$": {},
                        "conv_blocks.1.conv_pw$": {},
                        "conv_blocks.2.conv_dw$": {},
                        "conv_blocks.2.conv_pw$": {},
                        "conv_blocks.3.conv_dw$": {},
                        "conv_blocks.3.conv_pw$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_channel",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTLinear": {
                        "fc1$": {},
                        "kwargs": {
                            "n_levels": 256,
                            "quantize": "per_layer",
                            "init_clip": "max",
                            "learn_clip": true,
                            "symm_wts": true,
                            "tqt": true
                        }
                    },
                    "PACTUnsignedAct": {
                        "stem_block.relu$": {},
                        "conv_blocks.0.relu_dw$": {},
                        "conv_blocks.0.relu_pw": {},
                        "conv_blocks.1.relu_dw": {},
                        "conv_blocks.1.relu_pw": {},
                        "conv_blocks.2.relu_dw": {},
                        "conv_blocks.2.relu_pw": {},
                        "conv_blocks.3.relu_dw": {},
                        "conv_blocks.3.relu_pw": {},
                        "kwargs": {
                            "n_levels": 256,
                            "nb_std": 256,
                            "init_clip": "max",
                            "noisy": false,
                            "rounding": true,
                            "learn_clip": true,
                            "tqt": true,
                            "upper_percentile": 1.0,
                            "lower_percentile": 0.0,
                            "leaky": 0.0
                        }
                    },
                    "harmonize": {
                    "n_levels": [256, 256],
                    "noisy": false,
                    "rounding": true,
                    "force_out_eps": false,
                    "learn_clip": true,
                    "init_clip": "std",
                    "tqt": true,
                    "infer_outact": true,
                    "infer_sign": true
                    }
                }
            }
        }
    },
    "training": {
        "loss_fn": {
            "class": "CrossEntropyLoss",
            "kwargs": {}
        },
        "gd": {
            "opt": {
                "class": "SGD",
                "takes_net": false,
                "kwargs": {
                    "lr": 0.00075,
                    "momentum": 0.9,
                    "weight_decay": 4e-5
                }
            },
            "lr_sched": {
                "class": "HandScheduler",
                "kwargs": {
                    "schedule": {
                        "0": 1,
                        "4": 0.1,
                        "7": 0.01
                    }
                }
            }
        },
        "n_epochs": 12,
        "quantize": {
            "function": "get_pact_controllers",
            "kwargs": {
                "schedules": {
                    "linear": {
                        "0": ["start", "verbose_on"]
                    },
                    "activation": {
                        "1": ["start", "verbose_on"]
                    }
                },
                "kwargs_linear": {
                },
                "kwargs_activation": {
                    "init_clip_hi": 6.0
                }
            }

        }
    },
    "meters": {
        "train": {
            "compute_profiling_statistic": false,
            "compute_task_statistic": true
        },
        "valid": {
            "compute_profiling_statistic": true
        }
    }
}
